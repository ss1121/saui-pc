ss-display-flex($flex-flow = row nowrap, $justify-content = false, $align-items = false, $align-content = false)
  display: flex
  flex-flow: $flex-flow
  justify-content: $justify-content if($justify-content)
  align-items: $align-items if ( $align-items)
  align-content: $align-content if ( $align-content)

$color = #272E40
$borderColor = #E5E6ED
$theadColor = #272E40
$theadSize = 12px
$theadBg = #fff


.bootstrap-table 
  .table
    border: 1px solid $borderColor;
    border-collapse: collapse !important;
    border-radius: 1px;
    &:not(.table-condensed),&:not(.table-condensed) > tbody > tr > th,&:not(.table-condensed) > tfoot > tr > th,&:not(.table-condensed) > thead > tr > td,&:not(.table-condensed) > tbody > tr > td, &:not(.table-condensed) > tfoot > tr > td
      padding: 10px 8px;
    &:not(.table-condensed) > tbody > tr
      &:hover
        background: #F5F6FA;
      > td
        &:last-child
          white-space: nowrap
    

.fixed-table-container 
  position: relative;
  clear: both;
  // border-top: 1px solid $borderColor;
  // border-radius: 4px;
  table 
    width: 100%;
    color: $color
  td, th
    vertical-align: middle;
    box-sizing: border-box;
  thead
    color: $theadColor
    background: $theadBg
    font-weight: bold
    font-size: $theadSize
    th 
      height: 0;
      padding: 0;
      margin: 0;
      border-left: 1px solid $borderColor;
      text-align: left
      &:first-child
        border-left: none;
          // border-top-left-radius: 4px;
          // -webkit-border-top-left-radius: 4px;
          // -moz-border-radius-topleft: 4px;
  tbody
    td
      border-top: 1px solid $borderColor;
      border-left: @border-top
      word-wrap: break-word;      //lgh 20171216
      &:first-child
        border-left: none
    // tr
    //   &:first-child
    //     td
    //       border-top: none;
    // .selected td 
    // background-color: #f5f5f5;
    .disabled
      color: #ccc
  .bs-checkbox 
    text-align: center
    .th-inner
      padding: 8px 0;
  input[type="radio"], input[type="checkbox"]
    margin: 0 auto !important;
  .no-records-found 
    text-align: center;
    color: #999
    &:hover
      background: none
  .th-inner
    padding: 8px;
    line-height: 18px;
    vertical-align: top;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap
  .sortable 
    cursor: pointer;
    background-position: right;
    background-repeat: no-repeat;
    // padding-right: 30px;
  .both
    background-image: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABMAAAATCAQAAADYWf5HAAAAkElEQVQoz7X QMQ5AQBCF4dWQSJxC5wwax1Cq1e7BAdxD5SL+Tq/QCM1oNiJidwox0355mXnG/DrEtIQ6azioNZQxI0ykPhTQIwhCR+BmBYtlK7kLJYwWCcJA9M4qdrZrd8pPjZWPtOqdRQy320YSV17OatFC4euts6z39GYMKRPCTKY9UnPQ6P+GtMRfGtPnBCiqhAeJPmkqAAAAAElFTkSuQmCC');


.fixed-table-footer, .fixed-table-header
  overflow: hidden;


// .fixed-table-footer 
//   border-top: 1px solid $borderColor;

.fixed-table-body 
  // overflow-x: auto;        //20180118这个会影响到绝对定位的问题。供应列表
  // overflow-y: auto;
  height: 100%;

.fixed-table-pagination
  .pagination, .pagination-detail
    margin-top: 10px;
    margin-bottom: 10px;
    // margin-left: auto;
  // .pagination-detail 
  //   flex: 1;
  .pagination-info 
    line-height: 34px;
    margin-right: 5px;
    color: #999
  .pagination
    .pagination 
      margin: 0;
      ss-display-flex()
    a
      padding: 6px 12px;
      line-height: 1.428571429;
    >li>a, >li>span
      position: relative;
      padding: 5px 10px
      margin-left: -1px;
      line-height: 1.42857143;
      text-decoration: none;
      background-color: #fff;
      border: 1px solid #ccc;
      color: #666;
      display: inline-block
      min-width: 32px

.pagination
  >li
    >a
      &:hover
        color: #227ce1
    &:first-child, &:last-child
      >a,>span
        color: #333
    &:first-child
      >a,>span
        margin-left: 0;
        border-top-left-radius: 4px;
        border-bottom-left-radius: 4px
    &:last-child
      >a,>span
        border-top-right-radius: 4px;
        border-bottom-right-radius: 4px
    &.active
      >a, >span
        z-index: 2;
        color: #666;
        cursor: default;
        background-color: #ECF5FF;
        border-color: #227ce1
    &.disabled
      >a, >span
        color: #ccc;
        cursor: not-allowed;
        background-color: #f5f5f5;
        border-color: #E5E5E5


.fixed-table-pagination .page-list 
    display: inline-block;


.fixed-table-toolbar .bs-bars,
.fixed-table-toolbar .search,
.fixed-table-toolbar .columns {
    position: relative;
    margin-top: 10px;
    margin-bottom: 10px;
    line-height: 34px;
}

.fixed-table-pagination li.disabled a 
  pointer-events: none;
  cursor: default;
  color: #ccc

.fixed-table-loading 
  display: none;
  position: absolute;
  top: 42px;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 99;
  background-color: #fff;
  text-align: center;
  height: 37px;
  line-height: 37px;




